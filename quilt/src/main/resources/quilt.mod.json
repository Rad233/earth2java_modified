{
  "schema_version": 1,
  "mixin": [
    "earthtojava-common.mixins.json"
  ],
  "quilt_loader": {
    "group": "${group}",
    "id": "earthtojavamobs",
    "version": "${version}",
    "name": "earth2java",
    "description": "Bring mobs from Minecraft Earth to Minecraft Java Edition.",
    "authors": [
      "Slexom"
    ],
    "contact": {
      "homepage": "https://www.curseforge.com/minecraft/mc-mods/earth2java-fabric",
      "sources": "https://github.com/Slexom/earth2java",
      "issues": "https://github.com/Slexom/earth2java/issues"
    },
    "license": "MIT License",
    "icon": "assets/earthtojavamobs/logo.png",
    "environment": "*",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "slexom.earthtojava.fabric.Earth2JavaModQuilt"
      ],
      "client_init": [
        "slexom.earthtojava.quilt.Earth2JavaClientModQuilt"
      ],
      "mm:early_risers": [
        "slexom.earthtojava.quilt.Earth2JavaEarlyRiser"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": "1.19.4"
      },
      {
        "id": "architectury",
        "version": ">=8.0.0"
      }
    ]
  }
}
